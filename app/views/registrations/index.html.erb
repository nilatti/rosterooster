<p id="notice"><%= notice %></p>

<h1>Listing Registrations</h1>

<table>
  <thead>
    <tr>
      <th>Group</th>
      <th>Event</th>
      <th>Accommodation</th>
      <th>Person</th>
      <th>Paid</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registrations.each do |registration| %>
      <tr>
        <td><%= registration.group_id %></td>
        <td><%= registration.event_id %></td>
        <td><%= registration.accommodation_id %></td>
        <td><%= registration.person_id %></td>
        <td><%= registration.paid %></td>
        <td><%= registration.organization_id %></td>
        <td><%= link_to 'Show', registration %></td>
        <td><%= link_to 'Edit', edit_registration_path(registration) %></td>
        <td><%= link_to 'Destroy', registration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration', new_registration_path %>
